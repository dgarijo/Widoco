        <rdfs:domain rdf:resource="&software;Software"/>
        <category rdf:resource="http://ontosoft.org/softwareCategories#Trust"/>
        <rdfs:range rdf:resource="&prov;Agent"/>
    </owl:ObjectProperty>
    


