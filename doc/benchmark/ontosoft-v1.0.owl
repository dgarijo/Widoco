        <rdfs:range rdf:resource="&software;DevelopmentInformation"/>
        <rdfs:domain rdf:resource="&software;Software"/>
        <category rdf:resource="http://ontosoft.org/softwareCategories#Contribute"/>
    </owl:ObjectProperty>
    


    <!-- http://ontosoft.org/software#hasAdopter -->

    <owl:ObjectProperty rdf:about="&software;hasAdopter">
        <dc:description>Has the software been adopted in a project, organization or by a person?</dc:description>
